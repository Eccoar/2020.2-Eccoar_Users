{
   "compilerOptions": {
      "lib": [
         "es5",
         "es6"
      ],
      "target": "es5",
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "./build",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "baseUrl": ".",
      "sourceMap": true,
      "noImplicitAny": true,
   },
   "include":[
      "src/**/*.ts"
   ],
   "exclude":[
      "node_modules"
   ],
}